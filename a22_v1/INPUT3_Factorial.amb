# Amber Example 4:
  Testing numbers (ex: factorial) #
func main() {
	data := struct {
		i$, n$, f$ int
	}{}

	data.f$ = 1
	data.i$ = 1

	fmt.Print("Write a number: ")
	fmt.Scan(&data.n$)

	for data.i$ < data.n$ {
		data.f$ = data.f$ * data.i$
		data.i$ = data.i$ + 1
	}

	fmt.Println("Factorial:", data.f$)
}
